<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RelatorioActivity"
    android:background="@color/azul_roxo"
    >

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:clickable="true"
        android:onClick="telainicial"
        app:backgroundTint="@color/white"
        app:fabSize="mini"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_9524" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="RelatÃ³rios"
        android:textColor="#FFFFFF"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="100dp"
        android:background="@drawable/shapes"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/shapes"
        android:backgroundTint="@color/laranja_medio"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/btn_fev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="FEV"
        android:onClick="mes2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbl_ano" />

    <Button
        android:id="@+id/btn_mar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="20dp"
        android:text="MAR"
        android:onClick="mes3"
        app:layout_constraintStart_toEndOf="@+id/btn_fev"
        app:layout_constraintTop_toBottomOf="@+id/lbl_ano" />

    <Button
        android:id="@+id/btn_jul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="JUL"
        android:onClick="mes7"
        app:layout_constraintStart_toStartOf="@+id/btn_abr"
        app:layout_constraintTop_toBottomOf="@+id/btn_abr" />

    <Button
        android:id="@+id/btn_jan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="mes1"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="JAN"
        app:layout_constraintEnd_toStartOf="@+id/btn_fev"
        app:layout_constraintTop_toBottomOf="@+id/lbl_ano" />

    <Button
        android:id="@+id/btn_dez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="DEZ"
        android:onClick="mes12"
        app:layout_constraintStart_toStartOf="@+id/btn_set"
        app:layout_constraintTop_toBottomOf="@+id/btn_set" />

    <Button
        android:id="@+id/btn_set"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="SET"
        android:onClick="mes9"
        app:layout_constraintStart_toStartOf="@+id/btn_jun"
        app:layout_constraintTop_toBottomOf="@+id/btn_jun" />

    <Button
        android:id="@+id/btn_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="OUT"
        android:onClick="mes10"
        app:layout_constraintStart_toStartOf="@+id/btn_jul"
        app:layout_constraintTop_toBottomOf="@+id/btn_jul" />

    <Button
        android:id="@+id/btn_abr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="ABR"
        android:onClick="mes4"
        app:layout_constraintStart_toStartOf="@+id/btn_jan"
        app:layout_constraintTop_toBottomOf="@+id/btn_jan" />

    <Button
        android:id="@+id/btn_jun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="JUN"
        android:onClick="mes6"
        app:layout_constraintStart_toStartOf="@+id/btn_mar"
        app:layout_constraintTop_toBottomOf="@+id/btn_mar" />

    <Button
        android:id="@+id/btn_ago"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="AGO"
        android:onClick="mes8"
        app:layout_constraintStart_toStartOf="@+id/btn_mai"
        app:layout_constraintTop_toBottomOf="@+id/btn_mai" />

    <Button
        android:id="@+id/btn_mai"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="MAI"
        android:onClick="mes5"
        app:layout_constraintStart_toStartOf="@+id/btn_fev"
        app:layout_constraintTop_toBottomOf="@+id/btn_fev" />

    <Button
        android:id="@+id/btn_nov"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="NOV"
        android:onClick="mes11"
        app:layout_constraintStart_toStartOf="@+id/btn_ago"
        app:layout_constraintTop_toBottomOf="@+id/btn_ago" />

    <TextView
        android:id="@+id/lbl_ano"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:text="0000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_aumentar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="100dp"
        android:onClick="aumentar_ano"
        app:icon="@android:drawable/arrow_up_float"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toEndOf="@+id/lbl_ano"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_diminuir"
        android:layout_width="50dp"
        android:layout_height="0dp"
        android:layout_marginTop="100dp"
        android:onClick="diminuir_ano"
        app:backgroundTint="@color/azul_roxo"
        app:icon="@android:drawable/arrow_down_float"
        app:layout_constraintEnd_toStartOf="@+id/lbl_ano"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>